<?xml version="1.0" encoding="utf-8"?> 
<!DOCTYPE apt:template SYSTEM "policytemplate.dtd">
<apt:template>
   <resImport apt:packagePath="org.mozilla.apoc.policies.resource.firefox15_policies" />
     <category apt:name="Firefox1.5" apt:label="Firefox.firefox1.5" apt:inlineHelp="Firefox.cat_firefox_help">
      <category apt:name="Privacy" apt:label="Firefox.privacy" apt:inlineHelp="Firefox.cat_privacy_help">
        <page apt:name="Passwords" apt:label="Firefox.privacy.passwords" apt:inlineHelp="Firefox.privacy.page_passwords_help" apt:onlineHelp="/Firefox1.5/General/privPasswords">
         <section apt:name="SavedPasswords" apt:label="Firefox.privacy.saved_passwords">
             <property apt:dataPath="org.mozilla.signon/rememberSignons" apt:name="EnablePasswords" apt:label="Firefox.privacy.passwords_enable" oor:type="xs:boolean">
                <value>true</value>
                <visual apt:type="checkBox" />
             </property>
         </section>
     </page>
    </category>
   </category>
</apt:template>



